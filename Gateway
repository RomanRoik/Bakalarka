export class Gateway {
    private _id: string;
    private _incomingElements: string[];
    private _outgoingElements: string[];
    private _name: string;

    constructor(id: string, name: string, incomingElements: string[], outgoingElements: string[]) {
        this._id = id;
        this._incomingElements = incomingElements;
        this._outgoingElements = outgoingElements;
        this._name = name;
    }
    get id(): string {
        return this._id;
    }
    set id(id: string) {
        this._id = id;
    }
    get name(): string {
        return this._name;
    }
    set name(id: string) {
        this._name = name;
    }
    get incomingElements(): string[] {
        return this._incomingElements;
    }
    set incomingElements(incomingElements: string[]) {
        this._incomingElements = incomingElements;
    }
    get outgoingElements(): string[] {
        return this._outgoingElements;
    }
    set outgoingElements(outgoingElements: string[]) {
        this._outgoingElements = outgoingElements;
    }
}
